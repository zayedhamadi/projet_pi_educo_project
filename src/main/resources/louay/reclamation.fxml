<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pi_project.louay.Controller.reclamationController">
   <children>
      <TableView fx:id="tableView" layoutX="148.0" layoutY="45.0" prefHeight="222.0" prefWidth="427.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="40.0" text="Id" />
          <TableColumn fx:id="colTitre" text="Titre" />
            <TableColumn fx:id="colDescription" prefWidth="150.0" text="Description" />
            <TableColumn fx:id="colStatut" prefWidth="75.0" text="Statut" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER_RIGHT" layoutY="303.0" prefHeight="100.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Button fx:id="modifierBtn" mnemonicParsing="false" prefHeight="37.0" prefWidth="103.0" text="modifier">
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Button fx:id="supprimerBtn" mnemonicParsing="false" prefHeight="37.0" prefWidth="108.0" text="supprimer">
               <font>
                  <Font size="16.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
